{
  "name": "testmaker",
  "version": "1.0.0",
  "description": "Automated Test Analysis Tool",
  "type": "module",
  "scripts": {
    "analyze": "tsx src/cli/index.ts",
    "search": "tsx src/cli/index.ts",
    "dashboard": "tsx watch src/dashboard/server.ts",
    "supervisor": "tsx src/cli/supervisor.ts",
    "start:supervisor": "tsx src/cli/supervisor.ts",
    "worker": "tsx src/dashboard/worker.ts",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/",
    "regression:init": "tsx src/regression/cli.ts init",
    "regression:list": "tsx src/regression/cli.ts list",
    "regression": "tsx src/regression/cli.ts run",
    "record": "tsx src/recorder/cli.ts",
    "analyze:session": "tsx src/recorder/analyze-cli.ts",
    "benchmark": "tsx src/benchmark/compare-cli.ts",
    "calculate-coverage": "tsx src/scripts/calculate_coverage.ts",
    "db:sync": "tsx src/scripts/sync.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@playwright/test": "^1.57.0",
    "@types/chokidar": "^1.7.5",
    "@types/node": "^25.0.3",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.57.0",
    "pngjs": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "handlebars": "^4.7.8",
    "prisma": "^7.3.0",
    "sharp": "^0.34.5"
  }
}